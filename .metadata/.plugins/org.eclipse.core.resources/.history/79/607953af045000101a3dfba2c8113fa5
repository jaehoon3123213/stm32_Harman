#include "timewatch.h"

typedef struct{
	uint16_t msec;
	uint8_t sec;
	uint8_t min;
	uint8_t hour;
} timeWatch_t;

timeWatch_t timeWatch = {0};

void TimeWatch_IncTimeCallBack()
{

	if(timeWatch.msec < 1000 - 1) {
		timeWatch.msec ++;
		return;
	}
	timeWatch.msec = 0;

	else if (timeWatch.sec < 60 -1)
	{

	}
	timeWatch.msec;
};


