#include <stdint.h>



int main(void)
{
//	1. 페리페럴에 clock 넣기//
	*(volatile uint32_t *) 0x40023830 |= (1U << 0);//  RCC_AHB1ENR 해당위치의 비트만 1로 만들겠다
	*(volatile uint32_t *) 0x40020000 |= (1U << 10);//GPIOA_MODER
	*(volatile uint32_t *) 0x40020000 &= ~(1U << 11); // 11번 bit를 0




//	2. PA5를 output으로 설정//

//	3. set PA5 high//

//	4. set PA5 low//
	while(1)
	{

		*(volatile uint32_t *) 0x40020014 |= (1U << 5);//GPIOA_MODER

		*(volatile uint32_t *) 0x40020014 &= ~(1U << 5); // 11번 bit를 0

	}
}
