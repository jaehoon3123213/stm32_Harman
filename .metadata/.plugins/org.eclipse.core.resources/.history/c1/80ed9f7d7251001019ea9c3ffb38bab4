
#define QUE_SIZE   4


int front = 0;
int rear  = 0;


int quedata[QUE_SIZE] = {0};

int isQueFull() {

	return front == ((rear +1) % QUE_SIZE);
}

int isQueEmpty()
{
	return front == rear;
}

void enQue(int data)
{
	if(isQueFull())
	{
		return;
	}
	quedata[rear] = data;
	rear = (rear+1) % QUE_SIZE;
}

int deQue()
{
	if(isQueEmpty())
	{
		return;
	}
int data = queData[front];
front = (front+1) % QUE_SIZE;
return data;
}

int peekQue()
{
	if(isQueEmpty)
	{
		return;
	}
	return queData[front];
}
