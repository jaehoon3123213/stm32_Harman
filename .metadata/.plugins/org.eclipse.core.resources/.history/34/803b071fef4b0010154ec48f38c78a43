#include <stdint.h>

#define RCC_AHB1ENR *(volatile uint32_t *) 0x40023830
#define GPIOA_MODER *(volatile uint32_t *) 0x40020000
#define GPIOA_ODR  *(volatile uint32_t *) 0x40020014

void delay();

int main(void)
{
//	1. 페리페럴에 clock 넣기//
	RCC_AHB1ENR |= (1U << 0);//  RCC_AHB1ENR 해당위치의 비트만 1로 만들겠다
	GPIOA_MODER |= (1U << 10);//GPIOA_MODER
	GPIOA_MODER &= ~(1U << 11); // 11번 bit를 0




//	2. PA5를 output으로 설정//

//	3. set PA5 high//

//	4. set PA5 low//
	while(1)
	{

		GPIOA_ODR |= (1U << 5);//GPIOA_MODER
		delay();
		GPIOA_ODR &= ~(1U << 5); // 11번 bit를 0
		delay();


	}
}


}


void delay()
{
	for(int i = 0; i<100000; i++){}
}

