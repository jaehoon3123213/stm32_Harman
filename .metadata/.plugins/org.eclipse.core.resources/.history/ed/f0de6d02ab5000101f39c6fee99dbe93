#ifndef AP_STOPWATCH_H_
#define AP_STOPWATCH_H_

#include "stm32f4xx_hal.h"
#include "button.h"
#include "FND.h"
#include "TIM.h"

typedef struct{
	uint16_t msec;
	uint8_t sec;
	uint8_t min;
	uint8_t hour;
} stopWatch_t;

void  Stopwatch_excute();
void StopWatch_IncTimeCallBack();

