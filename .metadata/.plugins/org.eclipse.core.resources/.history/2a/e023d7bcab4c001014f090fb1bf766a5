#include <stdint.h>

#include "GPIO.h"
#include "LedBar.h"
#include "SystemClock.h"

int main(void)
{
	SysyemClock_init();
	LED_Bar_init();


	uint8_t data = 1;
	while(1)
	{
		LED_Bar_Write(data);
		delay(5);
		data = (data << 1) | (data >> 7);
	}
	return 0;



}
