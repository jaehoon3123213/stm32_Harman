#include "Presenter.h"


void Presenter_Init()
{
	Presenter_StopWatch_Init();
	Presenter_TimeWatch_Init();
}


void Presenter_Excute()
{
	eModestate_t state;

    state = Model_GetModeState();
	switch(state)
	{
	case S_TIME_WATCH_MODE:
		Presenter_TimeWatch_Excute();
		break;
	case S_STOP_WATCH_MODE:
		Presenter_StopWatch_Excute();
		break;
	}
	Presenter_StopWatch_Excute();
}

