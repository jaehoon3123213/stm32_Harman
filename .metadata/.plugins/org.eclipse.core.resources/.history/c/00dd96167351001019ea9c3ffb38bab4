
#define QUE_SIZE   4




typedef struct {
int front ;
int rear ;
int quedata[QUE_SIZE];
} Que_TypeDef;

void Que_Init(Que_TypeDef *q)
{
	q->front = 0;
	q->rear = 0;
}


int isQueFull() {

	return front == ((rear +1) % QUE_SIZE);
}

int isQueEmpty()
{
	return front == rear;
}

void enQue(int data)
{
	if(isQueFull())
	{
		return;
	}
	quedata[rear] = data;
	rear = (rear+1) % QUE_SIZE;
}

int deQue()
{
	if(isQueEmpty())
	{
		return;
	}
int data = queData[front];
front = (front+1) % QUE_SIZE;
return data;
}

int peekQue()
{
	if(isQueEmpty)
	{
		return;
	}
	return queData[front];
}
